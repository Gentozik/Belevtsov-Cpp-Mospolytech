#include <iostream>
#include <string>

using namespace std;

int main() {

    int x = 150;
    float y = 15.933;
    unsigned short z = 250;

    cout << "x = " << z << endl;
    cout << "y = " << y << endl;
    cout << "z = " << z << endl;

    const int day = 9;
    const string month = "March";
    const int year = 2004;
    cout << "Birthday date: " << day << "th of " << month << ", " << year << "\n";

    float f = 2.3;
    string w = "Windows";
    cout << f << " " << w;
}
